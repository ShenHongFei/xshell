{
    "name": "xshell",
    "description": "xshell is a shell designed to provide a brand new human-computer interaction experience.",
    "keywords": [
        "shell",
        "node",
        "repl",
        "interactive programming"
    ],
    "version": "0.0.5",
    "type": "commonjs",
    "main": "./extension.js",
    "bin": {
        "xshell": "./xshell.js"
    },
    "engines": {
        "node": ">=16.0.0",
        "vscode": ">=1.55.0"
    },
    "scripts": {
        "start": "node --title=xshell --inspect=0.0.0.0:8420 ./xshell.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc",
        "package": "vsce package",
        "publish:npm": "npm publish --access=public",
        "publish:extension": "vsce publish"
    },
    "author": {
        "name": "ShenHongFei",
        "email": "shf19961008@gmail.com",
        "url": "https://github.com/ShenHongFei"
    },
    "publisher": "ShenHongFei",
    "license": "MIT",
    "homepage": "https://github.com/ShenHongFei/xshell",
    "repository": {
        "type": "git",
        "url": "https://github.com/ShenHongFei/xshell"
    },
    "bugs": {
        "url": "https://github.com/ShenHongFei/xshell/issues"
    },
    "icon": "xshell.png",
    "extensionKind": [
        "workspace"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "xshell_repl",
                "title": "xshell_repl"
            }
        ],
        "keybindings": [
            {
                "command": "xshell_repl",
                "key": "ctrl+enter",
                "when": "editorTextFocus && (editorLangId == 'typescript' || editorLangId == 'javascript' || editorLangId == 'typescriptreact' || editorLangId == 'javascriptreact')"
            }
        ]
    },
    "dependencies": {
        "@koa/cors": "^3.1.0",
        "byte-size": "^7.0.1",
        "chardet": "^1.3.0",
        "cheerio": "^1.0.0-rc.6",
        "colors": "^1.4.0",
        "emoji-regex": "^9.2.2",
        "fs-extra": "^9.1.0",
        "fs-monkey": "^1.0.3",
        "iconv-lite": "^0.6.2",
        "koa": "^2.13.1",
        "koa-compress": "^5.0.1",
        "koa-useragent": "^4.0.0",
        "lodash": "^4.17.21",
        "memfs": "^3.2.2",
        "parse-multipart-data": "^1.0.0",
        "promise-retry": "^2.0.1",
        "puppeteer-core": "^9.0.0",
        "qs": "^6.10.1",
        "readdir-enhanced": "^6.0.4",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9",
        "stream-buffers": "^3.0.2",
        "strip-ansi": "^6.0.0",
        "tough-cookie": "^4.0.0",
        "trash": "^7.1.1",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4",
        "upath": "^2.0.1",
        "utf-8-validate": "^5.0.4"
    },
    "devDependencies": {
        "@types/chardet": "^0.8.0",
        "@types/cheerio": "^0.22.28",
        "@types/fs-extra": "^9.0.11",
        "@types/koa": "^2.13.1",
        "@types/koa-compress": "^4.0.1",
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.41",
        "@types/node-schedule": "^1.3.1",
        "@types/promise-retry": "^1.1.3",
        "@types/qs": "^6.9.6",
        "@types/request": "^2.48.5",
        "@types/request-promise-native": "^1.0.17",
        "@types/shortid": "^0.0.29",
        "@types/stream-buffers": "^3.0.3",
        "@types/vscode": "^1.55.0"
    }
}

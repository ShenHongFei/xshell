{
    "name": "xshell",
    "version": "0.0.14",
    "main": "./index.js",
    "bin": {
        "xshell": "./xshell.js"
    },
    "description": "xshell is a shell designed to provide a brand new human-computer interaction experience.",
    "keywords": [
        "shell",
        "node",
        "repl",
        "interactive programming"
    ],
    "type": "commonjs",
    "engines": {
        "node": ">=17.1.0",
        "vscode": ">=1.62.0"
    },
    "scripts": {
        "start": "node --title=xshell --inspect=0.0.0.0:8420 ./xshell.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc",
        "package": "vsce package",
        "publish:npm": "npm publish --access=public",
        "publish:extension": "vsce publish"
    },
    "author": {
        "name": "ShenHongFei",
        "email": "shen.hongfei@outlook.com",
        "url": "https://github.com/ShenHongFei"
    },
    "publisher": "ShenHongFei",
    "license": "MIT",
    "homepage": "https://github.com/ShenHongFei/xshell",
    "repository": {
        "type": "git",
        "url": "https://github.com/ShenHongFei/xshell"
    },
    "bugs": {
        "url": "https://github.com/ShenHongFei/xshell/issues"
    },
    "icon": "xshell.png",
    "extensionKind": [
        "workspace"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "xshell_repl",
                "title": "xshell_repl"
            }
        ],
        "keybindings": [
            {
                "command": "xshell_repl",
                "key": "ctrl+enter",
                "when": "editorTextFocus && (editorLangId == 'typescript' || editorLangId == 'javascript' || editorLangId == 'typescriptreact' || editorLangId == 'javascriptreact')"
            }
        ]
    },
    "dependencies": {
        "@koa/cors": "^3.1.0",
        "byte-size": "^8.1.0",
        "chalk": "^4.1.2",
        "chardet": "^1.4.0",
        "cheerio": "^1.0.0-rc.10",
        "emoji-regex": "^10.0.0",
        "fs-extra": "^10.0.0",
        "fs-monkey": "^1.0.3",
        "iconv-lite": "^0.6.3",
        "koa": "^2.13.4",
        "koa-compress": "^5.1.0",
        "koa-useragent": "^4.0.0",
        "lodash": "^4.17.21",
        "memfs": "^3.4.0",
        "promise-retry": "^2.0.1",
        "qs": "^6.10.1",
        "readdir-enhanced": "^6.0.4",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9",
        "resolve-path": "^1.4.0",
        "rimraf": "^3.0.2",
        "stream-buffers": "^3.0.2",
        "strip-ansi": "=6.0.0",
        "tough-cookie": "^4.0.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.2",
        "upath": "^2.0.1"
    },
    "devDependencies": {
        "@types/chardet": "^0.8.1",
        "@types/cheerio": "^0.22.30",
        "@types/fs-extra": "^9.0.13",
        "@types/koa": "^2.13.4",
        "@types/koa-compress": "^4.0.3",
        "@types/lodash": "^4.14.177",
        "@types/node": "^16.11.10",
        "@types/promise-retry": "^1.1.3",
        "@types/qs": "^6.9.7",
        "@types/request": "^2.48.7",
        "@types/request-promise-native": "^1.0.18",
        "@types/rimraf": "^3.0.2",
        "@types/stream-buffers": "^3.0.4",
        "@types/tampermonkey": "^4.0.5",
        "@types/vscode": "^1.62.0"
    }
}

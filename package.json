{
    "name": "xshell",
    "version": "0.0.24",
    "main": "./index.js",
    "bin": {
        "xshell": "./xshell.js",
        "i18n-scan": "./i18n/i18n-scan.js"
    },
    "description": "xshell is a shell designed to provide a brand new human-computer interaction experience.",
    "keywords": [
        "shell",
        "node",
        "repl",
        "i18n",
        "interactive programming"
    ],
    "type": "commonjs",
    "engines": {
        "node": ">=17.1.0",
        "vscode": ">=1.62.0"
    },
    "scripts": {
        "start": "node --title=xshell --inspect=0.0.0.0:8420 ./xshell.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc",
        "package": "vsce package",
        "publish:npm": "npm publish --access=public",
        "publish:extension": "vsce publish"
    },
    "author": {
        "name": "ShenHongFei",
        "email": "shen.hongfei@outlook.com",
        "url": "https://github.com/ShenHongFei"
    },
    "publisher": "ShenHongFei",
    "license": "MIT",
    "homepage": "https://github.com/ShenHongFei/xshell",
    "repository": {
        "type": "git",
        "url": "https://github.com/ShenHongFei/xshell"
    },
    "bugs": {
        "url": "https://github.com/ShenHongFei/xshell/issues"
    },
    "icon": "xshell.png",
    "extensionKind": [
        "workspace"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "xshell_repl",
                "title": "xshell_repl"
            }
        ],
        "keybindings": [
            {
                "command": "xshell_repl",
                "key": "ctrl+enter",
                "when": "editorTextFocus && (editorLangId == 'typescript' || editorLangId == 'javascript' || editorLangId == 'typescriptreact' || editorLangId == 'javascriptreact')"
            }
        ]
    },
    "dependencies": {
        "@babel/core": "^7.16.0",
        "@babel/parser": "^7.16.4",
        "@babel/traverse": "^7.16.3",
        "@koa/cors": "^3.1.0",
        "byte-size": "^8.1.0",
        "chalk": "^4.1.2",
        "chardet": "^1.4.0",
        "cheerio": "^1.0.0-rc.10",
        "cli-table3": "^0.6.0",
        "cli-truncate": "=2.1.0",
        "colors": "^1.4.0",
        "commander": "^8.3.0",
        "ejs": "^3.1.6",
        "emoji-regex": "^10.0.0",
        "fs-extra": "^10.0.0",
        "fs-monkey": "^1.0.3",
        "gulp-sort": "^2.0.0",
        "hash-string": "^1.0.0",
        "i18next-scanner": "^3.1.0",
        "i18next": "^21.6.0",
        "iconv-lite": "^0.6.3",
        "js-cookie": "^3.0.1",
        "koa-compress": "^5.1.0",
        "koa-useragent": "^4.0.0",
        "koa": "^2.13.4",
        "lodash": "^4.17.21",
        "map-stream": "0.0.7",
        "memfs": "^3.4.0",
        "ora": "=5.4.1",
        "promise-retry": "^2.0.1",
        "qs": "^6.10.2",
        "react-i18next": "^11.15.0",
        "react": "^17.0.2",
        "readdir-enhanced": "^6.0.4",
        "request-promise-native": "^1.0.9",
        "request": "^2.88.2",
        "resolve-path": "^1.4.0",
        "rimraf": "^3.0.2",
        "stream-buffers": "^3.0.2",
        "strip-ansi": "=6.0.0",
        "tough-cookie": "^4.0.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.3",
        "upath": "^2.0.1",
        "vinyl-fs": "^3.0.3",
        "vinyl": "^2.2.1"
    },
    "devDependencies": {
        "@babel/types": "^7.16.0",
        "@types/babel__traverse": "^7.14.2",
        "@types/chardet": "^0.8.1",
        "@types/cheerio": "^0.22.30",
        "@types/ejs": "^3.1.0",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/gulp-sort": "2.0.0",
        "@types/js-cookie": "^3.0.1",
        "@types/koa-compress": "^4.0.3",
        "@types/koa": "^2.13.4",
        "@types/lodash": "^4.14.178",
        "@types/node": "^16.11.12",
        "@types/promise-retry": "^1.1.3",
        "@types/qs": "^6.9.7",
        "@types/react": "^17.0.37",
        "@types/request-promise-native": "^1.0.18",
        "@types/request": "^2.48.7",
        "@types/rimraf": "^3.0.2",
        "@types/stream-buffers": "^3.0.4",
        "@types/tampermonkey": "^4.0.5",
        "@types/vinyl-fs": "^2.4.12",
        "@types/vscode": "^1.63.0"
    }
}
